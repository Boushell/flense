{
  "name": "flense",
  "version": "0.0.3",
  "description": "Convert PDF, DOCX, images to markdown. Document parsing SDK with OCR, table extraction, and real-time progress.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "prepublishOnly": "pnpm run build",
    "test:manual": "tsx test/manual.ts"
  },
  "keywords": [
    "pdf",
    "markdown",
    "pdf-to-markdown",
    "document-parser",
    "ocr",
    "pdf-parser",
    "docx",
    "pdf-extraction",
    "text-extraction",
    "image-to-text",
    "document-conversion",
    "table-extraction",
    "pdf-reader",
    "flense"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Boushell/flense.git"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.0",
    "eslint": "^9.39.1",
    "tsx": "^4.19.0",
    "typescript": "5.9.2"
  }
}
